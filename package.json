{
  "name": "ts-playground",
  "description": "A template for developing TypeScript projects, with sensible defaults",
  "version": "1.0.0",
  "author": {
    "name": "Peng Cao",
    "url": "https://github.com/caopengau/ts-playground"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yarn-upgrade-all": "^0.7.2"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js",
    "dist/**/*.js.map"
  ],
  "keywords": [
    "eslint",
    "javascript",
    "yarn",
    "prettier",
    "template",
    "typescript"
  ],
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest",
    "build": "tsc --build \"tsconfig.prod.json\"",
    "clean": "rimraf dist",
    "lint": "yarn lint:ts && yarn prettier:check && yarn type:check",
    "lint:ts": "eslint --ext .js,.ts .",
    "prettier:check": "prettier --check \"**/*.{js,json,md,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,ts,yml}\"",
    "type:check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2"
  }
}
